d":{"line":115,"column":0},"source":"views/pages/editProfile.js","original":{"line":72,"column":4}},{"name":"template","generated":{"line":115,"column":25},"source":"views/pages/editProfile.js","original":{"line":72,"column":11}},{"generated":{"line":115,"column":33},"source":"views/pages/editProfile.js","original":{"line":72,"column":4}},{"name":"App","generated":{"line":115,"column":35},"source":"views/pages/editProfile.js","original":{"line":72,"column":21}},{"name":"rootEl","generated":{"line":115,"column":48},"source":"views/pages/editProfile.js","original":{"line":72,"column":25}},{"generated":{"line":115,"column":54},"source":"views/pages/editProfile.js","original":{"line":72,"column":4}},{"generated":{"line":116,"column":0},"source":"views/pages/editProfile.js","original":{"line":73,"column":3}},{"generated":{"line":118,"column":0},"source":"views/pages/editProfile.js","original":{"line":9,"column":18}},{"generated":{"line":120,"column":15},"source":"views/pages/editProfile.js","original":{"line":76,"column":15}},{"name":"ProfileView","generated":{"line":120,"column":19},"source":"views/pages/editProfile.js","original":{"line":76,"column":19}},{"generated":{"line":120,"column":30},"source":"views/pages/editProfile.js","original":{"line":76,"column":15}}],"sources":{"views/pages/editProfile.js":"import App from './../../App'\nimport {html, render } from 'lit-html'\nimport {gotoRoute, anchorRoute} from './../../Router'\nimport Auth from './../../Auth'\nimport Utils from './../../Utils'\nimport UserAPI from './../../UserAPI'\nimport Toast from '../../Toast'\n\nclass ProfileView {\n  init(){    \n    document.title = 'Edit Profile'    \n    this.user = null\n    this.render()    \n    Utils.pageIntroAnim()\n    this.getUser()    \n  }\n\n  async getUser(){\n    try {\n      this.user = await UserAPI.getUser(Auth.currentUser._id)      \n      this.render()\n    }catch(err){      \n      Toast.show(err, 'error')\n    }\n  }\n\n  async updateProfileSubmitHandler(e){\n    e.preventDefault()\n    const formData = e.detail.formData\n    try {\n      const updatedUser = await UserAPI.updateUser(Auth.currentUser._id, formData)      \n      delete updatedUser.password        \n      this.user = updatedUser     \n      Auth.currentUser = updatedUser\n      this.render()\n      Toast.show('profile updated')\n    }catch(err){      \n      Toast.show(err, 'error')\n    }\n  }\n\n  render(){\n    const template = html`\n      <va-app-header title=\"Edit Profile\" user=${JSON.stringify(Auth.currentUser)}></va-app-header>\n      <div class=\"page-content\">        \n        ${(this.user == null) ? html`\n          <sl-spinner></sl-spinner>\n        `:html`\n          <sl-form class=\"page-form\" @sl-submit=${this.updateProfileSubmitHandler.bind(this)}>\n            <div class=\"input-group\">\n              <sl-input type=\"text\" name=\"firstName\" value=\"${this.user.firstName}\" placeholder=\"First Name\"></sl-input>\n            </div>\n            <div class=\"input-group\">\n              <sl-input type=\"text\" name=\"lastName\" value=\"${this.user.lastName}\" placeholder=\"Last Name\"></sl-input>\n            </div>\n            <div class=\"input-group\">\n              <sl-input type=\"text\" name=\"email\" value=\"${this.user.email}\" placeholder=\"Email Address\"></sl-input>\n            </div>\n            <div class=\"input-group\">\n              <label>Avatar</label><br>          \n              ${(this.user.avatar) ? html`\n                <sl-avatar image=\"${App.apiBase}/images/${this.user.avatar}\"></sl-avatar>\n              `: html`\n                <input type=\"file\" name=\"avatar\" />\n              `}\n            </div>\n            <sl-button type=\"primary\" submit>Update Profile</sl-button>\n          </sl-form>\n        `}\n      </div>\n    `\n    render(template, App.rootEl)\n  }\n}\n\nexport default new ProfileView()"},"lineCount":null}},"error":null,"hash":"4d205145d825fe4045ebe1f724b3be6a","cacheData":{"env":{}}}