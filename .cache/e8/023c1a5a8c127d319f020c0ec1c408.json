n  position: fixed;\n  background: #fff;\n  color: #777;\n  display: flex;\n  left: 0;\n  top:0;\n  right: 0;\n  bottom: 0;\n  align-items: center;\n  justify-content: center;\n  text-align: center;\n  .app-logo {\n    width: 180px;\n    display: block;\n    margin-bottom: 1em;\n  }\n}\n\n\n// helper classes ---------\n.calign {\n  text-align: center;\n}\n\n.large-text {\n  font-size: 1.6em;\n}\n\n.brand-color {\n  color: var(--brand-color);\n}\n\n// generic-entry ------------\n.generic-entry {\n  box-shadow: 0px 3px 5px rgba(0,0,0,0.07);\n  border-radius: 5px;\n  margin-bottom: .5em;\n  padding: 1em;\n  background: #fff;\n}\n\n// root ---------------\n#root {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;  \n  overflow: hidden;\n}\n\n// page --------------------\n.page-content {\n  position: absolute;\n  top: var(--app-header-height);\n  left: 0;\n  right: 0;\n  bottom: 0;\n  overflow-y: scroll;\n  padding: 1em;\n  &.no-pad {\n    padding: 0;\n  }\n}\n\n.page-centered {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n// sign in + sign up pages ----------\n.signinup-box,\n.signup-box {  \n  max-width: 320px;\n  text-align: center;\n}\n\n.signinup-logo {\n  width: 200px;\n  margin-bottom: 1em;\n}\n\n// guide page ---------------\n.guide-step {\n  padding: 1em;\n  margin: 1.5em auto;\n  max-width: 600px;\n  img {\n    width: 100%;\n    height: auto;\n  }\n}\n\n// RESPONSIVE - MOBILE -------------------\n@media all and (max-width: 768px){ \n\n  .signinup-box {\n    width: 90%;\n    max-width: none;\n  }\n}","scss/_forms.scss":"// Forms\n\n.input-group {\n  margin-bottom: .5em;\n}\n.page-form {\n  max-width: 700px;\n}","scss/_shoelace.scss":"// Shoelace\n\n// sl-dialog\nsl-dialog::part(title){\n  font-weight: 500;\n  font-size: 1.6em;\n}\n\n\n// RESPONSIVE - MOBILE -------------------\n@media all and (max-width: 768px){ \n\n  sl-dialog::part(panel) {\n    width: 100%;\n    height: 100%;\n    max-height: none;\n    max-width: none;\n  }\n}","scss/_toast.scss":"// Toast\n\n#toasts {\n  position: fixed;\n  left: 0;\n  right: 0;\n  top: 1em;  \n  z-index: 999;\n  display: flex;\n  justify-content: center;\n}\n\n.toast-entry {\n  background:#fff;\n  color: #333;  \n  border-radius: 200px;\n  box-shadow: 0px 3px 25px rgba(0,0,0,0.14);\n  padding: .7em 1.3em;\n  font-size: 1.1em;\n  margin-bottom: .25em;\n  transition: all .2 ease;\n  &.is-error {\n    background: rgb(212, 25, 93);\n    color:#fff;\n  }  \n}"},"lineCount":null}},"error":null,"hash":"00529260182280ac03e56e340c62ca75","cacheData":{"env":{}}}